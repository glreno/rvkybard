<!-- An arrow. Centered. It is ARROWLENGTH px long, ARROWWIDTH px wide, and pointed down unless rotated. -->
<!-- The plain arrow.svgt is length=16 and width=8 -->
<!-- expected parameters: ARROW (rotation in degrees), ARROWCOLOR (color), ARROWLENGTH, ARROWWIDTH -->
<g transform="rotate(@{ARROW})" >
<line x1=0 y1=@{ARROWLENGTH/-2} x2=0 y2=@{ARROWLENGTH/2} style="stroke:@{ARROWCOLOR}; stroke-width:2"/>
<line x1=@{ARROWWIDTH/-2} y1=0 x2=0 y2=@{ARROWLENGTH/2} style="stroke:@{ARROWCOLOR}; stroke-width:2"/>
<line x1=@{ARROWWIDTH/2} y1=0 x2=0 y2=@{ARROWLENGTH/2} style="stroke:@{ARROWCOLOR}; stroke-width:2"/>
</g>
