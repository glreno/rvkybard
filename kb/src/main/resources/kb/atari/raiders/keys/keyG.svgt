<svg width="@{W}" height="@{H}">

<circle fill="@{BORDC}" cx="8" cy="8" r="8"/>
<circle fill="@{BORDC}" cx="@{W-8}" cy="8" r="8"/>
<circle fill="@{BORDC}" cx="8" cy="@{H-8}" r="8"/>
<circle fill="@{BORDC}" cx="@{W-8}" cy="@{H-8}" r="8"/>
<rect fill="@{BORDC}" x="0" y="8" width="@{W}" height="@{H-16}"/>
<rect fill="@{BORDC}" x="8" y="0" width="@{W-16}" height="@{H}"/>

<circle fill="@{SHFBGC}" cx="12" cy="12" r="8"/>
<circle fill="@{SHFBGC}" cx="@{W-12}" cy="12" r="8"/>
<circle fill="@{BGC}" cx="12" cy="@{H-12}" r="8"/>
<circle fill="@{BGC}" cx="@{W-12}" cy="@{H-12}" r="8"/>

<rect fill="@{SHFBGC}" x="4" y="12" width="@{W-8}" height="@{H/3-14}"/>
<rect fill="@{BGC}" x="4" y="@{H/3+2}" width="@{W-8}" height="@{H*2/3-14}"/>
<rect fill="@{SHFBGC}" x="12" y="4" width="@{W-24}" height="@{H/3-8}"/>
<rect fill="@{BGC}" x="12" y="@{H/3+2}" width="@{W-24}" height="@{H*2/3-6}"/>

<text x="16" y="@{H-FS2*1.40+FIXLY}" dominant-baseline="middle" text-anchor="middle" font-size="@{FS2}" fill="@{TXTC}" font-family="sans-serif">@{name}</text>
<text x="@{W/2}" y="@{H/4-FS2*1.10+FIXSY}" dominant-baseline="middle" text-anchor="middle" font-size="@{FS2}" fill="@{TXTC}" font-family="sans-serif">@{S1}</text>
<text x="@{W/2}" y="@{H/4-FS2*.10+FIXSY}" dominant-baseline="middle" text-anchor="middle" font-size="@{FS2}" fill="@{TXTC}" font-family="sans-serif">@{S2}</text>
<!-- grid center lines -->
<line x1='@{W/2}' y1='@{H*.4}' x2='@{W/2}' y2='@{H*.7}' style='stroke:@{TXTC}; stroke-width:3;' />
<line x1='@{W*.1}' y1='@{H/2}' x2='@{W*.9}' y2='@{H/2}' style='stroke:@{TXTC}; stroke-width:3;' />

<!-- grid vertical lines from .1 to .9 -->
<line x1='@{W*.1}' y1='@{H*.4}' x2='@{W*.1}' y2='@{H*.7}' style='stroke:@{TXTC}; stroke-width:3;' />
<line x1='@{W*.2}' y1='@{H*.4}' x2='@{W*.2}' y2='@{H*.7}' style='stroke:@{TXTC}; stroke-width:3;' />
<line x1='@{W*.3}' y1='@{H*.4}' x2='@{W*.3}' y2='@{H*.7}' style='stroke:@{TXTC}; stroke-width:3;' />
<line x1='@{W*.4}' y1='@{H*.4}' x2='@{W*.4}' y2='@{H*.7}' style='stroke:@{TXTC}; stroke-width:3;' />
<line x1='@{W*.6}' y1='@{H*.4}' x2='@{W*.6}' y2='@{H*.7}' style='stroke:@{TXTC}; stroke-width:3;' />
<line x1='@{W*.7}' y1='@{H*.4}' x2='@{W*.7}' y2='@{H*.7}' style='stroke:@{TXTC}; stroke-width:3;' />
<line x1='@{W*.8}' y1='@{H*.4}' x2='@{W*.8}' y2='@{H*.7}' style='stroke:@{TXTC}; stroke-width:3;' />
<line x1='@{W*.9}' y1='@{H*.4}' x2='@{W*.9}' y2='@{H*.7}' style='stroke:@{TXTC}; stroke-width:3;' />

<!-- grid horizontal lines from .4 to .7 -->
<line x1='@{W*.1}' y1='@{H*.4}' x2='@{W*.9}' y2='@{H*.4}' style='stroke:@{TXTC}; stroke-width:3;' />
<line x1='@{W*.1}' y1='@{H*.6}' x2='@{W*.9}' y2='@{H*.6}' style='stroke:@{TXTC}; stroke-width:3;' />
<line x1='@{W*.1}' y1='@{H*.7}' x2='@{W*.9}' y2='@{H*.7}' style='stroke:@{TXTC}; stroke-width:3;' />

<!-- some dots -->
<rect fill="@{CTLBGC}" x="@{W*.14}" y="@{H*.44}" width="@{W*.02}" height="@{H*.02}"/>
<rect fill="@{CTLBGC}" x="@{W*.24}" y="@{H*.64}" width="@{W*.02}" height="@{H*.02}"/>
<rect fill="@{CTLBGC}" x="@{W*.34}" y="@{H*.44}" width="@{W*.02}" height="@{H*.02}"/>
<rect fill="@{CTLBGC}" x="@{W*.44}" y="@{H*.54}" width="@{W*.02}" height="@{H*.02}"/>
<rect fill="@{CTLBGC}" x="@{W*.63}" y="@{H*.43}" width="@{W*.02}" height="@{H*.02}"/>
<rect fill="@{CTLBGC}" x="@{W*.65}" y="@{H*.45}" width="@{W*.02}" height="@{H*.02}"/>
<rect fill="@{CTLBGC}" x="@{W*.84}" y="@{H*.64}" width="@{W*.02}" height="@{H*.02}"/>
<!-- big dot, maybe a starbase -->
<rect fill="@{TXTC}" x="@{W*.53}" y="@{H*.63}" width="@{W*.04}" height="@{H*.04}"/>

</svg>
