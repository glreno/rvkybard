<svg width="@{W}" height="@{H}">

<circle class='depress' fill="@{BORDC}" cx="8" cy="8" r="8"/>
<circle class='depress' fill="@{BORDC}" cx="@{W-8}" cy="8" r="8"/>
<circle class='depress' fill="@{BORDC}" cx="8" cy="@{H-8}" r="8"/>
<circle class='depress' fill="@{BORDC}" cx="@{W-8}" cy="@{H-8}" r="8"/>
<rect class='depress' fill="@{BORDC}" x="0" y="8" width="@{W}" height="@{H-16}"/>
<rect class='depress' fill="@{BORDC}" x="8" y="0" width="@{W-16}" height="@{H}"/>

<circle fill="@{SHFBGC}" cx="12" cy="12" r="8"/>
<circle fill="@{SHFBGC}" cx="@{W-12}" cy="12" r="8"/>
<circle fill="@{BGC}" cx="12" cy="@{H-12}" r="8"/>
<circle fill="@{BGC}" cx="@{W-12}" cy="@{H-12}" r="8"/>

<rect fill="@{SHFBGC}" x="4" y="12" width="@{W-8}" height="@{H/3-14}"/>
<rect fill="@{BGC}" x="4" y="@{H/3+2}" width="@{W-8}" height="@{H*2/3-14}"/>
<rect fill="@{SHFBGC}" x="12" y="4" width="@{W-24}" height="@{H/3-8}"/>
<rect fill="@{BGC}" x="12" y="@{H/3+2}" width="@{W-24}" height="@{H*2/3-6}"/>

<text x="16" y="@{H-FS2*1.40+FIXLY}" dominant-baseline="middle" text-anchor="middle" font-size="@{FS2}" fill="@{TXTC}" font-family="sans-serif">@{name}</text>
<text x="@{W/2}" y="@{H/4-FS2*1.10+FIXSY}" dominant-baseline="middle" text-anchor="middle" font-size="@{FS2}" fill="@{TXTC}" font-family="sans-serif">@{S1}</text>
<text x="@{W/2}" y="@{H/4-FS2*.10+FIXSY}" dominant-baseline="middle" text-anchor="middle" font-size="@{FS2}" fill="@{TXTC}" font-family="sans-serif">@{S2}</text>

<!-- Bomb body : rect from face center, rotated up a few degrees -->
<g transform='translate(@{W*.3},@{H*.75})'>
    <g transform='rotate(-12)'>
        <g transform='translate(0,-20)'>
            <rect fill="@{SHFBGC}" x='0' y='0' width='70' height='40'/>
            <!-- top fin -->
            <polygon points="
                70,-10 70,0 50,0
            " style="fill:@{CTLBGC}" />
            <!-- bottom fin -->
            <polygon points="
                70,50 70,40 50,40
            " style="fill:@{CTLBGC}" />
            <!-- side fin -->
            <line x1="50" y1="20" x2="70", y2="20" stroke="@{CTLBGC}" stroke-width="2" />
        </g>
    </g>
</g>

<!-- Bomb face : center is W*.3, H*.75 -->
<circle fill="@{SHFBGC}" cx="@{W*.3}" cy="@{H*.75}" r="19"/>
<!-- Glasses -->
<line x1="@{W*.21}" y1="@{H*.68}" x2="@{W*.3}" y2="@{H*.66}" style='stroke:#000;stroke-width:1;' />
<line x1="@{W*.35}" y1="@{H*.68}" x2="@{W*.3}" y2="@{H*.66}" style='stroke:#000;stroke-width:1;' />
<circle cx="@{W*.17}" cy="@{H*.7}" r="5" fill="@{BORDC}" />
<circle cx="@{W*.17}" cy="@{H*.7}" r="5" style='stroke:#000;stroke-width:1;fill:none' />
<circle cx="@{W*.21}" cy="@{H*.7}" r="5" fill="@{BORDC}" />
<circle cx="@{W*.21}" cy="@{H*.7}" r="5" style='stroke:#000;stroke-width:1;fill:none' />
<!-- Mortarboard -->
<polygon points="
     @{W*.15},@{H*.56},
     @{W*.43},@{H*.70},
     @{W*.54},@{H*.51},
     @{W*.27},@{H*.46},
" style="fill:@{BORDC};stroke:@{BGC};stroke-width:1" />

<!-- Center point of mortarboard: (.15+.54)/2=.345; (.51+.56)/2=.535
Center point of front bot line:   (.15+.43)/2=.29;  (.56+.70)/2=.63
 -->
<circle cx="@{W*.345}" cy="@{H*.535}" r="2" fill="@{CTLBGC}" />
<line x1="@{W*.345}" y1="@{H*.535}" x2="@{W*.29}" y2="@{H*.63}" stroke="@{CTLBGC}" />
<line x1="@{W*.29}" y1="@{H*.85}" x2="@{W*.29}" y2="@{H*.63}" stroke="@{CTLBGC}" />
<circle cx="@{W*.29}" cy="@{H*.75}" r="2" fill="@{CTLBGC}" />
<line x1="@{W*.29}" y1="@{H*.75}" x2="@{W*.29}" y2="@{H*.83}" stroke="@{CTLBGC}" stroke-width="3" />


</svg>
